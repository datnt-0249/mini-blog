<%= form_with model: current_user.active_relationships.find_by(followed: @user),
              html: {method: :delete} do |f| %>
  <%= f.submit t(".submit"), class: "w-full text-white cursor-pointer bg-blue-700 hover:bg-blue-800 focus:outline-none font-medium rounded-full text-md px-5 py-2.5 text-center" %>
<% end %>
